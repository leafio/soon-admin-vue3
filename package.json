    "element-plus": "^2.8.5",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "perfect-scrollbar": "^1.5.5",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^4.1.1",
    "soon-fetch": "^1.0.0",
    "soon-i18n-vue": "^1.0.5",
    "sortablejs": "^1.15.3",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/node": "^22.7.5",
    "@types/nprogress": "^0.2.3",
    "@types/sortablejs": "^1.15.8",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "autoprefixer": "^10.4.20",
