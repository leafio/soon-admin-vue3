{
  "name": "soon-admin-vue3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "concurrently \"soon-mock -p 9090\" \"vite --mode mock\" ",
    "mock": "soon-mock -p 9090",
